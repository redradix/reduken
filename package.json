{
  "name": "reduken",
  "version": "2.0.6",
  "description": "Generic redux reducers, actions and selectors.",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/redradix/reduken.git"
  },
  "scripts": {
    "test": "jest",
    "test:dev": "jest --watch",
    "test:coverage": "jest --coverage",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "clear": "rm -r batch entities hash lib list requests index.js",
    "prebuild": "npm run clear",
    "build": "npm test && npm run build:js && npm run build:types",
    "build:types": "./generate-types.sh",
    "build:js": "babel src -d . --extensions \".ts,.js\" --minified --ignore src/**/*.test.js",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "reducer",
    "redux",
    "redis",
    "hash"
  ],
  "jest": {
    "testURL": "https://www.redradix.com"
  },
  "author": "redradix",
  "license": "ISC",
  "dependencies": {
    "ramda": "^0.26.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.7",
    "@babel/core": "^7.7.7",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.7.7",
    "@babel/preset-env": "^7.7.7",
    "@babel/preset-typescript": "^7.7.7",
    "babel-jest": "^24.7.1",
    "jest": "^24.7.1",
    "typescript": "^3.7.4"
  },
  "bugs": {
    "url": "https://github.com/redradix/reduken/issues"
  },
  "homepage": "https://github.com/redradix/reduken/#readme"
}
