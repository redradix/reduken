{
  "name": "reduken",
  "version": "2.0.6",
  "description": "Generic redux reducers, actions and selectors.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "build": "babel src --extensions .js,.ts --out-dir dist && tsc",
    "test": "jest --config jestconfig.json",
    "test:dev": "npm test -- --watch",
    "test:coverage": "npm test -- --coverage",
    "prettier": "prettier --ignore-path .gitignore \"src/**/*.(ts|js|json)\"",
    "format": "npm run prettier -- --write",
    "check-format": "npm run prettier -- --list-different",
    "lint": "eslint --ignore-path .gitignore .",
    "check-types": "tsc",
    "validate": "npm-run-all --parallel check-types check-format lint build",
    "prepare": "npm run build",
    "prepublishOnly": "npm-run-all test lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "keywords": [
    "reducer",
    "redux",
    "redis",
    "hash"
  ],
  "author": "redradix",
  "license": "ISC",
  "dependencies": {
    "ramda": "^0.26.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.4",
    "@babel/preset-env": "^7.8.4",
    "@babel/preset-typescript": "^7.8.3",
    "@types/jest": "^24.0.24",
    "@typescript-eslint/eslint-plugin": "^2.20.0",
    "@typescript-eslint/parser": "^2.20.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "husky": "^4.2.3",
    "jest": "^24.9.0",
    "lint-staged": "^10.0.7",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.19.1",
    "ts-jest": "^24.2.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.7.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/redradix/reduken.git"
  },
  "bugs": {
    "url": "https://github.com/redradix/reduken/issues"
  },
  "homepage": "https://github.com/redradix/reduken/#readme"
}
